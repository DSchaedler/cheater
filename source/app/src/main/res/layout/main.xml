<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	>
	<LinearLayout
		android:id="@+id/linear4"
		android:layout_width="wrap_content"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:orientation="vertical"
		android:layout_gravity="center_horizontal"
		>
		<TextView
			android:id="@+id/howbadtextview"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="How bad did you cheat\?"
			android:textSize="16sp"
			android:textStyle="bold"
			android:textColor="#000000"
			android:layout_gravity="center_horizontal"
			/>
		<Button
			android:id="@+id/badchoicebutton"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="#FFF176"
			android:text="Made a bad choice"
			android:textSize="12sp"
			android:textColor="#000000"
			/>
		<Button
			android:id="@+id/overatebutton"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="#FFB74D"
			android:text="Ate too much"
			android:textSize="12sp"
			android:textColor="#000000"
			/>
		<Button
			android:id="@+id/sugarysnackbutton"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="#FF8A65"
			android:text="Had a sugary snack"
			android:textSize="12sp"
			android:textColor="#000000"
			/>
		<LinearLayout
			android:id="@+id/linear5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal"
			>
			<TextView
				android:id="@+id/badchoicelabel"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:text="Bad Choices:"
				android:textSize="12sp"
				android:textColor="#000000"
				/>
			<TextView
				android:id="@+id/badchoicesoutput"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:text="TextView"
				android:textSize="12sp"
				android:textColor="#000000"
				/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal"
			>
			<TextView
				android:id="@+id/overatelabel"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:text="Overate:"
				android:textSize="12sp"
				android:textColor="#000000"
				/>
			<TextView
				android:id="@+id/overateoutput"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:text="TextView"
				android:textSize="12sp"
				android:textColor="#000000"
				/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear7"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal"
			>
			<TextView
				android:id="@+id/sugarysnacklabel"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:text="Sugary Snacks:"
				android:textSize="12sp"
				android:textColor="#000000"
				/>
			<TextView
				android:id="@+id/sugarysnackoutput"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:text="TextView"
				android:textSize="12sp"
				android:textColor="#000000"
				/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
